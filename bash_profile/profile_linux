#!/usr/bin/bash

export DOTFILE_ROOT=~/.mine_dotfiles
export PROFILE_ROOT=$DOTFILE_ROOT/bash_profile

. $PROFILE_ROOT/scripts/start

# Source NVM
. $PROFILE_ROOT/scripts/nvm

# Source android
cd $PROFILE_ROOT/scripts
. ./path_exports
cd - > /dev/null

source ~/.local/bin/virtualenvwrapper.sh >/dev/null

pip3 completion --bash >/dev/null

